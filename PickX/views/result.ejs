<!doctype html>
<html class="no-js" lang="en" dir="ltr">
<head>
  <meta charset="utf-8">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>PickX - Application Results</title>
  <link rel="stylesheet" href="/stylesheets/foundation.css">
  <link rel="stylesheet" href="/stylesheets/app.css">
  <link rel="stylesheet" href="/stylesheets/result.css">
  <!-- Navigation -->
  <nav class="scrollhide-nav">
    <div class="top-bar">
      <div class="top-bar-left" style="margin-left:5%;">
        <ul class="dropdown menu" data-dropdown-menu>
          <li><a href="/"><img class="orbit-image" src="/images/logo.png" style="width:150px; height:80px"></a></li>
          <li style="padding-top:3%">
            <a href="#" style="color: #FFFFFF;">Categories</a>
            <ul class="menu vertical">
              <li><a href="#" style="color: #FFFFFF;">College</a></li>
              <li><a href="/listUniversity" style="color: #FFFFFF;">Undergraduates</a></li>
              <li><a href="#" style="color: #FFFFFF;">Postgraduate Courses</a></li>
            </ul>
          </li>
          <li style="padding-top:3%"><a href="#" style="color: #FFFFFF;">About Us</a></li>
          <li style="padding-top:3%"><a href="#" style="color: #FFFFFF;">FAQ</a></li>
        </ul>
      </div>
      <div class="top-bar-right" style="margin-right:10%;">
        <ul class="menu">
          <li><input type="search" placeholder="Search"></li>
          <form action="/showApplications">
            <li><button type="submit" class="button">Search</button></li>
          </form>
          <li><button type="button" class="button" style="margin-left:10%; width:100%">Sign Out</button></li>
        </ul>
      </div>
    </div>
  </nav>
</head>

<body>
<table class="table-expand" style="margin:10%; width:80%">
  <thead>
    <tr class="table-expand-row">
      <th width="200">ID</th>
      <th>Application Status</th>
      <th>Gender</th>
      <th class="text-right" width="150">Rank</th>
      <th width="150">CGPA</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < data.length ; i++){%>
    <tr class="table-expand-row" data-open-details>
      <td><%= data[i]['uid']%></td>
      <td><%= data[i]['id']%></td>
      <td><%= data[i]['gender']%></td>
      <td class="text-right"><%= data[i]['rank']%></td>
      <td><%= data[i]['cgpa']%><span class="expand-icon"></span></td>
    </tr>
    <% } %>
    

  </tbody>
</table>
<script src="/javascripts/vendor/jquery.js"></script>
  <script src="/javascripts/vendor/what-input.js"></script>
  <script src="/javascripts/vendor/foundation.js"></script>
  <script src="/javascripts/app.js"></script>
</body>

<div class="three-column-footer-contact-form-container">
  <footer class="three-column-footer-contact-form" data-equalizer data-equalize-by-row="true">
    <div class="footer-left" data-equalizer-watch>
      <div class="baseline">
        <div class="contact-details">
          <h6>Contact details</h6>
          <p><i class="fa fa-phone fa-lg" aria-hidden="true"></i> 01234 567890</p>
          <p><a href="#"><i class="fa fa-envelope-o" aria-hidden="true"></i> Contact us</a></p>
          <p><i class="fa fa-map-marker fa-lg" aria-hidden="true"></i> Street, City, County, Country</p>
        </div>
        <div class="newsletter">
          <div class="input-group">
            <h8>Sign up for our newsletter</h8>

            <input class="input-group-field" type="email" placeholder="Email address" style="margin-bottom:5%;">
          </div>
          <a class="button expanded" href="#" style="background-color:#6495ED">Submit</a>
        </div>
      </div>
    </div>
    <div class="footer-center" data-equalizer-watch>
      <div class="baseline">
        <div class="newsletter">
          <h6>Contact form</h6>
          <div>
            <input class="input-group-field" type="text" placeholder="Name" style="margin-bottom:2%;">
            <input class="input-group-field" type="email" placeholder="Email address" style="margin-bottom:2%;">
            <textarea placeholder="Message" style="margin-bottom:2%;"></textarea>
          </div>
          <a class="button expanded" href="#" style="background-color:#6495ED">Submit</a>
        </div>
      </div>
    </div>
    <div class="footer-right" data-equalizer-watch>
      <div class="baseline">
        <img class="thumbnail" src="/images/filler.jpg">
        <h6>Opening times</h6>
        <p>Mon - Fri 9:00am - 5:00pm</p>
        <p>Sat 9:00am - 8:00pm</p>
        <p>Sun 9:00am - 4:00pm</p>
        <div class="social">
          <i class="fa fa-facebook-square fa-2x" aria-hidden="true"></i>
          <i class="fa fa-twitter-square fa-2x" aria-hidden="true"></i>
          <i class="fa fa-google-plus-square fa-2x" aria-hidden="true"></i>
          <i class="fa fa-linkedin-square fa-2x" aria-hidden="true"></i>
        </div>
      </div>
    </div>
  </footer>
</div>

